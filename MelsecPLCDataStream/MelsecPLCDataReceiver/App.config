<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <runtime>
        <legacyCorruptedStateExceptionsPolicy enabled="true"/>
        <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
            <probing privatePath="libs" />
        </assemblyBinding>
    </runtime>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <appSettings>
        <add key="Port" value="8240" />
        <add key="DeviceName" value="D" />
        <add key="StartPos" value="1800" />
        <add key="KeepaliveTime" value="1000" />
        <add key="KeepaliveInterval" value="100" />
    </appSettings>
</configuration>